#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/node_modules/tellki-rabbitmq-performance-monitor/bin/tellki-rabbitmq-performance-monitor" "$@"
  ret=$?
else 
  node  "$basedir/node_modules/tellki-rabbitmq-performance-monitor/bin/tellki-rabbitmq-performance-monitor" "$@"
  ret=$?
fi
exit $ret